  </server>
  <profile id='CommunitySurveys' installLocation='{{ feb_root_folder }}'>
    <data key='cic.selector.arch' value='x86_64'/>
    <data key='user.profile.was.location,com.ibm.nitro.iim' value='{{ was_install_location }}'/>
    <data key='user.profile.was.profile.name,com.ibm.nitro.iim' value='AppSrv01'/>
    <data key='user.profile.was.host,com.ibm.nitro.iim' value='{{ dmgr_hostname }}'/>
    <data key='user.profile.was.port,com.ibm.nitro.iim' value='8879'/>
    <data key='user.profile.was.admin.name,com.ibm.nitro.iim' value='{{ was_username }}'/>
    <data key='user.profile.was.server.name,com.ibm.nitro.iim' value="Util-{{ hostvars[groups['cnx_was_servers'][0]]['inventory_hostname'].split('.') }}"/>
